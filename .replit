
modules = ["nodejs-20"]
hidden = [".config", "package-lock.json"]
run = "npm run dev"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
run = ["sh", "-c", "npm run dev"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 5173
externalPort = 8000
exposeLocalhost = true
